# Default .gitignore for Node.js projects
node_modules/
.next/
out/
build/
.DS_Store
*.pem
.env*.local

# Ignore other pages
src/app/onboarding/
src/app/premium/
src/app/dashboard/
src/app/default-couple/
src/app/privacy/
src/app/(app)/

# Ignore development files
coverage/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/versions
*.tsbuildinfo
next-env.d.ts

# Keep only the landing page and essential files
!src/app/page.tsx
!public/
!src/components/
!styles/
!next.config.js
!package.json
!package-lock.json
!tsconfig.json
!tailwind.config.js
!postcss.config.js
